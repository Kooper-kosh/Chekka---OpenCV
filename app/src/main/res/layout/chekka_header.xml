<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="80dp"
    android:id="@+id/customToolbarID"
    android:padding="9dp"
    tools:context=".Chekka"
    android:background="@drawable/rectangle_top_navigation">

    <LinearLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:orientation="horizontal"
        tools:ignore="MissingConstraints">
        <ImageView
            android:id="@+id/profiletologout"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            app:srcCompat="@drawable/ic_profile"/>
    </LinearLayout>
    <ImageView
        android:visibility="visible"
        android:id="@+id/header_faq_CH"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="10dp"
        app:srcCompat="@drawable/ic_error1"/>
        // FOR HOME AND DASHBOARD WITH MEOW
        <FrameLayout
            android:id="@+id/forhomeanddash_CH"
            android:layout_width="20dp"
            android:layout_height="50dp"
            android:layout_marginLeft="30dp"
            android:visibility="visible">

        <TextView
            android:id="@+id/TextViewCHEKKA_CH"
            android:visibility="invisible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="sans-serif-condensed"
            android:layout_marginTop="4dp"
            android:text="CHEKKA"
            android:textSize="25dp"
            android:textColor="@color/white"
            android:layout_marginLeft="20dp" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/fordash_CH"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:layout_marginLeft="30dp"
            android:visibility="invisible">
            <TextView
                android:visibility="invisible"
                android:id="@+id/TextViewChanging_CH"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:fontFamily="sans-serif-condensed"
                android:layout_marginTop="4dp"
                android:text=""
                android:textSize="25dp"
                android:textColor="@color/white"
                android:layout_marginLeft="20dp" />
            <ImageView
                android:id="@+id/header_backfordash_CH"
                android:visibility="invisible"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginTop="4dp"
                android:layout_marginRight="50dp"
                app:srcCompat="@drawable/ic_back"/>
        </FrameLayout>



        <FrameLayout
            android:id="@+id/forsettings_CH"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:visibility="invisible">
            <TextView
                android:id="@+id/TextViewSETTINGS_CH"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:fontFamily="sans-serif-condensed"
                android:layout_marginTop="4dp"
                android:text="SETTINGS"
                android:textSize="25dp"
                android:textColor="@color/white"
                android:layout_marginLeft="20dp" />
            <ImageView
                android:id="@+id/header_back_CH"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginTop="4dp"
                android:layout_marginLeft="250dp"
                app:srcCompat="@drawable/ic_back"/>

        </FrameLayout>



</RelativeLayout>